{
  "name": "flairverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./server",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "browserslist": "npx browserslist",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:chrome": "cypress run --browser chrome --reporter json",
    "cy:firefox": "cypress run --browser firefox --reporter json",
    "cy:edge": "cypress run --browser edge --reporter json",
    "cy:clear-reps:wins": "del .\\reporters\\reports\\junit\\*",
    "cy:clear-reps:unix": "rm -r reporters/reports/junit/*",
    "jest:run": "jest --silent=true",
    "jest:watch": "jest --watch --silent=false",
    "jest:ci": "jest --ci"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "express": "^4.17.3",
    "next": "12.1.0",
    "next-i18next": "7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "^0.6.1",
    "sass": "^1.49.9",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.23",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.6",
    "cypress": "^9.5.0",
    "cypress-multi-reporters": "^1.5.0",
    "http-proxy-middleware": "^2.0.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "junit-merge": "^2.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.0",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.0",
    "typescript": "4.5.5"
  }
}
