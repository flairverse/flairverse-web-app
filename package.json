{
  "name": "flairverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ./server",
    "start": "npm run dev",
    "build": "next build",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:chrome": "cypress run --browser chrome --reporter json",
    "cy:firefox": "cypress run --browser firefox --reporter json",
    "cy:edge": "cypress run --browser edge --reporter json",
    "cy:clear-reps:wins": "del .\\reporters\\reports\\junit\\*",
    "cy:clear-reps:unix": "rm -r reporters/reports/junit/*",
    "jest:run": "jest --silent=true",
    "jest:watch": "jest --watch --silent=false",
    "jest:ci": "jest --ci"
  },
  "dependencies": {
    "@giphy/js-fetch-api": "^4.1.2",
    "@giphy/react-components": "^5.6.1",
    "@svgr/webpack": "^6.2.1",
    "antd": "^4.21.4",
    "axios": "^0.26.0",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "next": "12.1.0",
    "next-i18next": "7.0.1",
    "next-plugin-antd-less": "^1.8.0",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.16",
    "react-sticky-box": "^1.0.1",
    "recoil": "^0.6.1",
    "styled-components": "^5.3.3",
    "ts-mixer": "^6.0.1"
